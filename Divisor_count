// from the o(n) to o(sqrt(n)) given programme

#include<bits/stdc++.h>
using namespace std;
void divisorcount( int n ) {
  int Count = 0;
  for ( int i = 1; i*i<=n; i++) {
    if ( i*i==n) Count++;
    else if ( n%i==0) Count+=2;
  }
  cout<<Count<<endl;
}
int main() {
    int n;
    cin>>n;
    divisorcount(n);
    return 0;
}

